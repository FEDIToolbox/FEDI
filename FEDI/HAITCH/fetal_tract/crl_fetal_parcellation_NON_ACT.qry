##################################################################################################
# Queries for different tracts
##################################################################################################

#Cingulum bundle
CG.side = (
    wm_Cingulum_Mid.side and
    (ctx_Frontal_Sup.side or 
                ctx_Frontal_Sup_Orb.side or 
                ctx_Rectus.side or
                ctx_Paracentral_Lobule.side or
                ctx_Precuneus.side)
    not in hemisphere.opposite
    not in medial_of(Thalamus.side)
    not in lateral_of(Thalamus.side)
    not in Brainstem
    not in (Lentiform.side or Caudate.side or Thalamus.side)
    )

#Middle longitudinal fascicle
MLF.side = (
    (
        (temporal.side and anterior_of(Amygdala.side)) or
        ctx_Temporal_Sup.side or
        ctx_Temporal_Pole_Sup.side
    )
    and (inferior_parietal_lobule.side or superior_parietal_lobule.side)
    not in wm_occipital.side
    not in hemisphere.opposite
)

#Inferior longitudinal fascicle
ILF.side = (
    (temporal.side and anterior_of(Hippocampus.side))
    and (occipital.side)
    not in (wm_parietal.side or wm_frontal.side)
    not in ctx_ParaHippocampal.side
    not in hemisphere.opposite
)

#Inferior occipito-frontal fascicle for NoACT Tracking
IFO.side = (
     (
        inferiorfrontalgyrus.side or
        ctx_Frontal_Mid_Rost.side or 
        orbitofrontalgyrus.side      
    )
    and (occipital.side)
    not in hemisphere.opposite
)

#Uncinate fascicle for Tracking without anatomical constraints
UF.side = (
    (
     (
        ctx_Frontal_Inf_Orb.side or
        orbitofrontalgyrus.side
    )
    and
     (
        temporal.side and anterior_of(Amygdala.side))
    )
    not in hemisphere.opposite
)

#Cortico-spinal tract
CST.side = (
    (endpoints_in(Brainstem)
    and
    (ctx_Precentral.side)
    )
    not in hemisphere.opposite
)


#Superior cerebellar peduncle
SCP.side = (
    endpoints_in(Thalamus.side)
    and
    endpoints_in(Cerebellum.side)
    not in hemisphere.opposite
)

#Middle cerebellar peduncle
MCP = (
    endpoints_in(Cerebellum.right)
    and
    endpoints_in(Cerebellum.left)
    and
    Brainstem             # should be pons/Pontine_nuclei, but freesurfer has only Brain_stem and not more fine grained parcellation
)

#Inferior cerebellar peduncle
ICP.side = (
    endpoints_in(Cerebellum.side)
    and
    endpoints_in(Brainstem)
    not in hemisphere.opposite
)

#Fronto-pontine tract
FPT.side = (
    (
    endpoints_in(Brainstem)
    and
    (ctx_Frontal_Sup.side)
    )
    not in hemisphere.opposite
)

#Parieto‚Äêoccipital pontine tract:
POPT.side = (
    (
    endpoints_in(Brainstem)
    and
    (ctx_Parietal_Sup.side or ctx_Postcentral.side)
    )
    not in hemisphere.opposite
)

#Optic radiation
OR.side = (
    endpoints_in(Thalamus.side)
    and
    (ctx_Lingual.side or ctx_Calcarine.side or ctx_Cuneus.side or ctx_Lat_Occipital.side)
    not in hemisphere.opposite
)

#Anterior Thalamic Radiation
ATR.side = (
    endpoints_in(thalamus.side)
    and
    (ctx_Frontal_Inf_Tri.side or
    ctx_Frontal_Inf_Orb.side or
    ctx_Frontal_Mid_Rost.side or
    ctx_Frontal_Sup_Orb.side or
    ctx_Frontal_Mid_Orb.side)
    and Internal_Capsule.side
    not in hemisphere.opposite
)

#Superior Thalamic Radiation
STR.side = (
    endpoints_in(Thalamus.side)
    and
    (ctx_Paracentral_Lobule.side)
    not in hemisphere.opposite
)

#Corpus Callosum (CC) in one big part
CC = (
    (ctx.left) and
    (ctx.right) and
    CorpusCallosum and
    #Removes majority of diagonal fibers
    (
        (
            (ctx_frontal.left)
            and
            (ctx_frontal.right)
        )
        or
        (
            (ctx_postal.left)
            and
            (ctx_postal.right)
        )
    )
)

#Rostrum
CC_1 = (
    endpoints_in(orbitofrontalgyrus.left)
    and endpoints_in(orbitofrontalgyrus.right
    and CorpusCallosum)
)

#Genu
CC_2 = (
    endpoints_in(prefrontal.left) 
    and endpoints_in(prefrontal.right)
    and CorpusCallosum
)

#Rostral body (Premotor)
CC_3 = (
    endpoints_in(premotor.left)
    and endpoints_in(premotor.right)
    and CorpusCallosum
)

#Anterior midbody (Primary Motor)
CC_4 = (
    endpoints_in(motor.left) 
    and endpoints_in(motor.right)
    and CorpusCallosum
)

#Posterior midbody (Primary Somatosensory)
CC_5 = (
    endpoints_in(
        ctx_Postcentral.left or
        ctx_Cingulum_Post.left or
        ctx_Paracentral_Lobule.left
    )
    and endpoints_in(
        ctx_Postcentral.right or
        ctx_Cingulum_Post.right or
        ctx_Paracentral_Lobule.right
    )
    and CorpusCallosum
)

#Isthmus NEW (more complete)
CC_6 = (
    endpoints_in(
        superior_temporal_lobule.left or
        posterior_parietal_lobule.left or
        superior_parietal_lobule.left or
        inferior_parietal_lobule.left
    )
    and endpoints_in(
        superior_temporal_lobule.right or
        posterior_parietal_lobule.right or
        superior_parietal_lobule.right or
        inferior_parietal_lobule.right
    )
    and CorpusCallosum
)

#Splenium (Occipital)
CC_7 = (
    endpoints_in(
        occipital.left or
        inferior_temporal_lobule.left
    )
    and endpoints_in(
        occipital.right or
        inferior_temporal_lobule.right
    )
    and CorpusCallosum
)


#Superior longitudinal fascicle I
SLF_I.side = (
    (ctx_Parietal_Sup.side)
    and
    endpoints_in(
        middlefrontalgyrus.side or
        ctx_Frontal_Sup.side
    )
    not in hemisphere.opposite
)

#Superior longitudinal fascicle II
SLF_II.side = (
    endpoints_in(
        ctx_Parietal_Inf.side or
        ctx_Lat_Occipital.side
    )
    and
    endpoints_in(
        middlefrontalgyrus.side or
        ctx_Frontal_Sup.side
    )
    not in hemisphere.opposite
)

#Superior longitudinal fascicle III
SLF_III.side = (
    (ctx_SupraMarginal.side)
    and endpoints_in(inferiorfrontalgyrus.side)
    not in hemisphere.opposite
    )

#Arcuate fascicle
AF.side = (
    endpoints_in(
        inferiorfrontalgyrus.side or
        middlefrontalgyrus.side or
        ctx_Precentral.side
    )
    and endpoints_in(temporal.side)
    not in hemisphere.opposite
)

#Thalamo-prefrontal
T_PREF.side = (
    endpoints_in(Thalamus.side) and endpoints_in(prefrontal.side)
    not in hemisphere.opposite
)

#Thalamo-premotor
T_PREM.side = (
    endpoints_in(Thalamus.side) and endpoints_in(premotor.side)
    not in hemisphere.opposite
)

#Thalamo-precentral
T_PREC.side = (
    endpoints_in(Thalamus.side) and (ctx_Precentral.side)
    not in hemisphere.opposite
)

#Thalamo-postcentral
T_POSTC.side = (
    endpoints_in(Thalamus.side) and (ctx_Postcentral.side)
    not in hemisphere.opposite
)

#Thalamo-parietal
T_PAR.side = (
    endpoints_in(Thalamus.side) and endpoints_in(parietal.side)
    not in hemisphere.opposite
)

#Thalamo-occipital
T_OCC.side = (
    endpoints_in(Thalamus.side) and endpoints_in(occipital.side)
    not in hemisphere.opposite
)

#Striato-fronto-orbital
ST_FO.side = (
    endpoints_in(striatum.side) and endpoints_in(orbitofrontalgyrus.side)
    not in hemisphere.opposite
)

#Striato-prefrontal
ST_PREF.side = (
    endpoints_in(striatum.side) and endpoints_in(prefrontal.side)
    not in hemisphere.opposite
)

#Striato-premotor
ST_PREM.side = (
    endpoints_in(striatum.side) and endpoints_in(premotor.side)
    not in hemisphere.opposite
)

#Striato-precentral
ST_PREC.side = (
    endpoints_in(striatum.side) and (ctx_Precentral.side)
    not in hemisphere.opposite
)

#Striato-postcentral
ST_POSTC.side = (
    endpoints_in(striatum.side) and (ctx_Postcentral.side)
    not in hemisphere.opposite
)

#Striato-parietal
ST_PAR.side = (
    endpoints_in(striatum.side) and endpoints_in(parietal.side)
    not in hemisphere.opposite
)

#Striato-occipital
ST_OCC.side = (
    endpoints_in(striatum.side) and endpoints_in(occipital.side)
    not in hemisphere.opposite
)